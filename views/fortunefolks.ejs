                <!-- <span class="big-circle1"></span> -->
                <!-- <span class="big-circle2"></span>     -->
                <img src="./images/shape.png" class="square" alt="" />
                <div class="form">
                  <div class="contact-info">
                    <h3 class="title">Contact Us</h3>
                    <div class="info">
                      <div class="information">
                        <img src="./images/contact-location.png" alt="location">
                        <p>126 CV Raman Lucknow-227301 , U.P , India</p>
                      </div>
                      <div class="information">
